{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.2.0",
    "generators": {
      "generator": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:5064/swagger/v1/swagger.json",
        "output": "src/backend/openapi",
        "globalProperty": "",
        "ignoreFileOverride": "#{cwd}/.openapi-generator-ignore",
        "removeOperationIdPrefix": true,
        "skipValidateSpec": true,
        "apiNameSuffix": "",
        "modelPackage": "dtos",
        "apiPackage": "apis",
        "packageName": "",
        "modelNamePrefix": "",
        "modelNameSuffix": "",
        "additionalProperties": {
          "withInterfaces": false,
          "withSeparateModelsAndApi": true
        }
      }
    }
  }
}
